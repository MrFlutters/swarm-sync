{
  "name": "swarm-sync",
  "version": "0.0.1",
  "description": "GitOps for Docker Swarm",
  "main": "src/index.js",
  "repository": "git@bitbucket.org:1dayio/microservice-reservations.git",
  "author": "Kevin Bennett <kevinb@1day.io>",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=10.4"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon --exec babel-node src/index.js",
    "build": "rimraf ./dist && babel -d ./dist ./src -s --copy-files",
    "test": "cross-env NODE_ENV=test mocha --require @babel/register --recursive ./src/ --exit",
    "pretty": "prettier --single-quote true --trailing-comma all --arrow-parens always --write \"src/**/*.js\"",
    "precommit": "lint-staged && yarn run lint",
    "lint": "eslint src/**",
    "start:dist": "node dist/index.js"
  },
  "lint-staged": {
    "*.{js,json,md}": [
      "yarn run pretty",
      "git add"
    ]
  },
  "dependencies": {
    "apollo-errors": "^1.9.0",
    "apollo-server-express": "^1.3.6",
    "body-parser": "^1.18.3",
    "compare-versions": "^3.4.0",
    "docker-registry-client": "^3.3.0",
    "dockerode": "^2.5.8",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "getenv": "^0.7.0",
    "graphql": "^0.13.2",
    "graphql-tools": "^3.0.2",
    "helmet": "^3.12.1",
    "js-yaml": "^3.12.1",
    "lodash": "^4.17.10",
    "minimatch": "^3.0.4",
    "mongoose": "^5.1.5",
    "node-cleanup": "^2.1.2",
    "project-version": "^1.0.0",
    "request": "^2.87.0",
    "request-promise": "^4.2.2",
    "request-promise-native": "^1.0.5",
    "shelljs": "^0.8.3",
    "simple-git": "^1.107.0",
    "tmp": "^0.0.33",
    "winston": "2.4.3",
    "winston-papertrail": "^1.0.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.51",
    "@babel/core": "^7.0.0-beta.51",
    "@babel/node": "^7.0.0-beta.51",
    "@babel/preset-env": "^7.0.0-beta.51",
    "@babel/register": "^7.0.0-beta.51",
    "chai": "^4.1.2",
    "cross-env": "^5.1.6",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.12.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.17.5",
    "prettier": "^1.13.5",
    "rimraf": "^2.6.2"
  }
}
